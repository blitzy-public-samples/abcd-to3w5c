# Node Environment
# Defines test environment for conditional configuration
VITE_NODE_ENV=test

# API Configuration
# Test API endpoint URL for mock API interactions
# Using localhost:3000 as specified in api.constants.ts
VITE_API_URL=http://localhost:3000/api/v1

# Auth0 Configuration
# Test authentication settings with mock Auth0 service
VITE_AUTH0_DOMAIN=test-habittracker.auth0.com
VITE_AUTH0_CLIENT_ID=test_client_id_123456789
VITE_AUTH0_AUDIENCE=http://localhost:3000/api/v1
VITE_AUTH0_REDIRECT_URI=http://localhost:5173/callback

# Analytics & Monitoring
# Test environment tracking IDs
VITE_ANALYTICS_ID=G-TEST123456
VITE_SENTRY_DSN=http://localhost:9000/test-project/123456

# Test Configuration
# Enable/disable test-specific features
VITE_TEST_MOCK_ENABLED=true
VITE_TEST_COVERAGE_ENABLED=true

# API Rate Limiting for Tests
# Based on rate limits from api.constants.ts
VITE_API_RATE_LIMIT_HABITS_LIST=100
VITE_API_RATE_LIMIT_HABITS_CREATE=60
VITE_API_RATE_LIMIT_HABITS_UPDATE=60
VITE_API_RATE_LIMIT_HABITS_LOG=120
VITE_API_RATE_LIMIT_ANALYTICS=30

# Test Data Configuration
# Mock data settings for consistent test scenarios
VITE_TEST_USER_EMAIL=test@habittracker.com
VITE_TEST_USER_PASSWORD=Test123!@#
VITE_TEST_MOCK_LATENCY=50

# Security Settings for Tests
# Test-specific security configurations
VITE_TEST_JWT_SECRET=test_jwt_secret_key_123
VITE_TEST_ENCRYPTION_KEY=test_encryption_key_456

# Feature Flags for Testing
# Toggle specific features in test environment
VITE_TEST_FEATURE_OFFLINE_MODE=true
VITE_TEST_FEATURE_NOTIFICATIONS=true
VITE_TEST_FEATURE_ANALYTICS=true

# Test Database Configuration
# Mock database settings for test isolation
VITE_TEST_DB_HOST=localhost
